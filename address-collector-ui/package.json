    "babel-core": "^5.6.15",
    "babel-eslint": "^4.0.10",
    "babel-loader": "^5.2.2",
    "babel-plugin-react-autoprefix": "^0.2.6",
    "css-loader": "^0.16.0",
    "eslint": "^1.2.0",
    "eslint-plugin-react": "^3.2.3",
    "extract-text-webpack-plugin": "^0.8.2",
